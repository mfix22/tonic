{
  "name": "ranger",
  "public": true,
  "type": "docker",
  "alias": "api.reporanger.com",
  "env": {
    "NODE_ENV": "production",
    "LOG_LEVEL": "info",
    "PRIVATE_KEY": "@issues_private_key",
    "APP_ID": "@issues_app_id",
    "WEBHOOK_SECRET": "@issues_secret",
    "REDIS_HOST": "@issues_redis_host",
    "REDIS_PORT": "@issues_redis_port",
    "REDIS_PASSWORD": "@issues_redis_password",
    "SENTRY_DSN": "@ranger_sentry_dsn"
  },
  "features": {
    "cloud": "v2"
  }
}
