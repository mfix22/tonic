{
  "name": "ranger",
  "version": "0.0.37",
  "description": "",
  "author": "Mike Fix",
  "license": "UNLICENSED",
  "repository": "https://github.com/mfix22/ranger.git",
  "scripts": {
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./index.js",
    "test": "jest",
    "lint": "eslint src",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "redis:clear": "redis-cli flushall",
    "deploy": "git push heroku master",
    "site": "cd site && live-server"
  },
  "dependencies": {
    "@sentry/node": "^5.6.1",
    "analytics-node": "^3.4.0-beta.1",
    "async-sema": "^3.0.0",
    "bad-words": "^3.0.3",
    "bee-queue": "^1.2.2",
    "lodash.merge": "^4.6.2",
    "moment": "^2.24.0",
    "ms": "^2.1.1",
    "probot": "^9.2.19",
    "rexrex": "^1.3.0",
    "smee-client": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.3",
    "jest": "^24.4.0",
    "lint-staged": "^9.2.1",
    "live-server": "^1.2.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "prettier --write",
      "git add"
    ]
  }
}
