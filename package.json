{
  "name": "ranger",
  "version": "0.0.3",
  "description": "",
  "author": "Mike Fix <mike@dawnlabs.io> (dawnlabs.io)",
  "license": "UNLICENSED",
  "repository": "https://github.com/dawnlabs/ranger.git",
  "scripts": {
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./index.js",
    "test": "jest",
    "lint": "eslint src",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "redis:clear": "redis-cli flushall",
    "deploy": "git push heroku master"
  },
  "dependencies": {
    "@sentry/node": "4.4.2",
    "airtable": "^0.5.8",
    "bee-queue": "^1.2.2",
    "moment": "^2.23.0",
    "ms": "^2.1.1",
    "probot": "^7.4.0",
    "smee-client": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^5.10.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.2.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "prettier --write",
      "git add"
    ]
  }
}
